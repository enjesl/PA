{
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "types": [
            "node",
            "playwright"
        ], 
        "baseUrl": ".",
        "paths": {
            "@commands": [
                "commands/index.ts"
            ],
            "@locators": [
                "locators/index.ts"
            ],
            "@pages": [
                "pages/index.ts"
            ],
            "@data": [
                "data"
            ],
            "@utils": [
                "utils"
            ],
        }
    }
}